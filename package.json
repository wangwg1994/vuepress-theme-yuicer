    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "5.2.3",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuicer/vuepress-theme-yuicer.git"
