  "main": ".eslintrc.js",
  "scripts": {
    "lint": "lint-staged",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
