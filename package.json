{
  "name": "@yuicer/vuepress-theme-yuicer",
  "version": "0.0.9",
  "description": "theme for vuepress",
  "main": "index.js",
  "scripts": {
    "lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm publish"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuicer/vuepress-theme-yuicer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yuicer/vuepress-theme-yuicer/issues"
  },
  "homepage": "https://github.com/yuicer/vuepress-theme-yuicer#readme",
  "dependencies": {
    "@vuepress/plugin-blog": "^1.0.0-alpha.47",
    "@vuepress/plugin-medium-zoom": "^1.0.0-alpha.47",
    "@vuepress/plugin-pagination": "^1.0.0-alpha.47",
    "@vuepress/plugin-back-to-top": "^1.0.0-alpha.47",
    "@vuepress/plugin-google-analytics": "^1.0.0-alpha.47",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0"
  }
}
